<div id="{{ page.id }}" class="content">
  <div class="flex-images">
    {% for img in site.data.art %}
      <div class="item" data-w="{{ img.w }}" data-h="{{ img.h }}"><img src="{{ img.src }}"></div>
    {% endfor %}
  </div>

  {{ css('css/fleximg') }}
  {{ js('js/fleximg') }}

  <script>
    new flexImages({selector: '.flex-images', rowHeight: 360});
  </script>
</div>
